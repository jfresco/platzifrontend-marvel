{
  "scripts": {
    "jade": "jade --watch --pretty lib/*.jade --out .",
    "styl": "stylus --use nib --compress --watch lib/app.styl --out public/css",
    "serve": "http-server .",
    "babel": "babel --watch lib --out-dir public",
    "browserify": "browserify -t babelify lib/app.js > public/app.js"
  },
  "dependencies": {
    "babel": "^5.5.5",
    "http-server": "^0.8.0",
    "jade": "^1.10.0",
    "nib": "^1.1.0",
    "stylus": "^0.51.1",
    "browserify": "9.0.8",
    "babelify": "6.1.2",
    "page": "1.6.3",
    "jquery": "2.1.4"
  }
}
